block content
  main
    //- <br class="banner__br-no-mob"
    +banner('--m-vertically')
      +banner-item('--discounts-ny')
        +banner-wrapper('img/banner_discounts_ny.png', true)
          +banner-h1(' Акция «Новогодние покупки с&nbsp;АТБ»', '--mob-size-l')
          +banner-text('Кэшбэк 10% за&nbsp;покупки на&nbsp;маркетплейсах')
          +banner-foot
            +ui-button('Подробнее', 'banner, --m-size-l')(href='#')

      +banner-item('--discounts')
        +banner-wrapper('img/top_ban_sale.png', true)
          +banner-h1(' Акции, скидки и&nbsp;программа лояльности', '--mob-size-l')

    .page-content
      +section('--mt0')
        .inner
          +great-percentages-ban(greatPercentagesBanData, '--discounts')

      +section#section-club
        +section-head
          .inner
            +ui-h2('АТБ Клуб — личный кабинет программы ', 'section-head')
              span.text-nowrap лояльности
                +ui-tooltip(' ', '--va-baseline')
                  p
                    b Для дебетовых карт
                    |  с&nbsp;тарифными планами: АТБ 2.0, Кошелек, Большой кошелек, Кэшбэк
                  p
                    b Для кредитных карт
                    |  (с&nbsp;1&nbsp;ноября 2023&nbsp;года) с&nbsp;тарифными планами: Абсолютный ноль, Мои правила, Ставка&nbsp;19, Фреш, 90 даром

        +tabs-head(tabsHead2, '', '--gradient-border')
        +tabs-main
          +tabs-main__item(true)
            +section('--mt0')
              .inner
                +club-offer
          +tabs-main__item#tab-lk
            +section('--mt0')
              .inner
                +club-offer('lk')
          +tabs-main__item()
            +section('--mt0')
              .inner
                +tel-instruction(telInstruction2, '--img-l, --club-instructions')
